                    <option>EAN_8</option>
                    <option>EAN_13</option>
                    <option>UPC_A</option>
                    <option>UPC_E0</option>
                    <option>UPC_E1</option>
                    <option>RM4SCC</option>
                    <option>UCCEAN128</option>
                    <option>QRCode</option>
                    <option>Code49</option>
