@model WebApiExplorer.Models.ControlsClass.Function
<div class="settings-panel-content">
    <div style="float:left;width:100%">
        <div style="float:left"><h4>@Html.Raw(Resources.Barcode.BarcodeGenerating_Parameters)</h4></div>
        <div style="float:right">
            <button style="margin-top:3px;padding: 4px;" class="btn btn-primary"
                    onclick="showBarcode('@Model.FunctionName', '@(System.Configuration.ConfigurationManager.AppSettings["WebAPIService"] + Model.FunctionUrl.Substring(1))');">
                @Html.Raw(Resources.Barcode.BarcodeGenerating_GenerateBarcode)
            </button>
        </div>
    </div>
    <div>
        <div class="settings-panel settings-panel-basic">
            <h4>@Html.Raw(Resources.Barcode.BarcodeGenerating_BacisSettings)</h4>
            <div class="param-item">
                <label class="param-lable parameter"><b>@Html.Raw(Resources.Barcode.BarcodeGenerating_Parameter)</b></label>
                <label class="param-lable value"><b>@Html.Raw(Resources.Barcode.BarcodeGenerating_Value)</b></label>
                <label class="param-lable description"><b>@Html.Raw(Resources.Barcode.BarcodeGenerating_Description)</b></label>
            </div>
            <div class="param-item">
                <label class="param-lable parameter">@Html.Raw(Resources.Barcode.BarcodeGenerating_BacisType)</label>
                <select class="param-value" name="Type" defaultvalue="Png">
                    <option selected="selected">Png</option>
                    <option>Jpeg</option>
                    <option>Bmp</option>
                    <option>Gif</option>
                    <option>Tiff</option>
                </select>
                <label class="param-lable description">@Html.Raw(Resources.Barcode.BarcodeGenerating_BacisTypeDesc)</label>
            </div>
            <div class="param-item">
                <label class="param-lable parameter">@Html.Raw(Resources.Barcode.BarcodeGenerating_BacisText)</label>
                <input class="param-value" type="text" name="Text" value="1234567890" />
                <label class="param-lable description">@Html.Raw(Resources.Barcode.BarcodeGenerating_BacisTextDesc)</label>
            </div>
            <div class="param-item">
                <label class="param-lable parameter">@Html.Raw(Resources.Barcode.BarcodeGenerating_BacisCodeType)</label>
