</div>
<script>
    function showBarcode(id, url) {
        var fullUrl = showResult(id, url);
        $("#result" + id).attr("src", fullUrl);
        $("#downloadLink").attr("href",fullUrl);

        var userAgent = navigator.userAgent.toLowerCase(),
            isEdge = userAgent.indexOf("edge/") > 0,
            isIE11 = /trident/.test(userAgent),
            isIE = isEdge || isIE11 || /msie/.test(userAgent);

        if(!isIE && $(".settings-panel-basic select[name='Type']").val()==="Tiff" ) {
            $("#result" + id).hide();
            $("#messageInfo").show();
        }
        else {
            $("#result" + id).show();
            $("#messageInfo").hide();
        }
    }
</script>