@using C1.Web.Mvc.Chart
@{
    ImageExportOptions optionsModel = ViewBag.Options;
    ViewBag.DemoSettings = true;
}

<script type="text/javascript">
    function xFunc(value) {
        return Math.cos(value * 5);
    }
    function yFunc(value) {
        return Math.sin(value * 7);
    }
</script>
@(Html.C1().FlexChart().Legend(Position.Right).Id(optionsModel.ControlId).Height(300)
.Series(ses =>
{
    ses.AddParametricFunctionSeries("ParametricFunctionSeries")
        .Max(2 * Math.PI)
        .SampleCount(1000)
        .XFunc("xFunc")
        .YFunc("yFunc");
}))

@section Settings{
    @Html.Partial("_ImageExportOptions", optionsModel)
}

@section Description{
    @Html.Raw(Resources.Wijmo5FlexChart.ParametricFunctionSeries_Text0)
}