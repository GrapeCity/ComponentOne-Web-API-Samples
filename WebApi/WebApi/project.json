{
  "dependencies": {
    "Microsoft.AspNetCore.Mvc": "1.0.0",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.0",
    "Microsoft.Extensions.Configuration.FileExtensions": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.Extensions.Logging": "1.0.0",
    "Microsoft.Extensions.Logging.Console": "1.0.0",
    "Microsoft.Extensions.Logging.Debug": "1.0.0",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.0",
    "System.Data.SqlClient": "4.1.0",
    "C1.AspNetCore.Api": "1.0.20213.304",
    "C1.AspNetCore.Api.BarCode": "1.0.20213.304",
    "C1.AspNetCore.Api.DataEngine": "1.0.20213.304",
    "C1.AspNetCore.Api.Document": "1.0.20213.304",
    "C1.AspNetCore.Api.Excel": "1.0.20213.304",
    "C1.AspNetCore.Api.Image": "1.0.20213.304",
    "C1.AspNetCore.Api.Pdf": "1.0.20213.304",
    "C1.AspNetCore.Api.Report": "1.0.20213.304",
     "AWSSDK.Core" :"3.3.31.4",
     "AWSSDK.S3" :"3.3.31.12",
     "C1.AspNetCore.Api.Cloud" :"1.0.20213.304",
     "Dropbox.Api" :"4.9.4",
     "Google.Apis" :"1.25.0",
     "Google.Apis.Drive.v3" :"1.25.0.862",
     "Microsoft.AspNetCore.StaticFiles" :"1.1.3",
     "WindowsAzure.Storage" :"8.7.0" 
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview2-final"
  },
  "frameworks": {
    "net452": {}
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "copyToOutput": [
      "wwwroot/**/*.*",
      "phantomjs.exe"
    ],
    "embed": "GCDTLicenses.xml",
    "define": [ "ASPNETCORE" ]
  },
  "publishOptions": {
    "include": [
      "wwwroot",
      "Views",
      "Areas/**/Views",
      "appsettings.json",
      "web.config",
      "phantomjs.exe"
    ],
    "publishExclude": [
      "**.user",
      "**.vspscc"
    ]
  },
  "scripts": {
    "postpublish": [
      "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%"
    ]
  }
}
